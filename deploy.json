{
  "server": "zozi@ocean-zb1",
  "server_dir": "/home/zozi",
  "repo": "git://github.com/zozi/zozibot.git",
  "prj_git_relative_dir": "",
  "branch": "master",
  "force_regenerate_git_dir": false,
  "shared_dirs": [
    "node_modules",
    "db",
    "pids"
  ],
  "prerun": [
    "npm cache clear",
    "npm cache clean",
    "npm install",
    "npm test",
    "ln -nfs ~/shared/bin/.hubotrc bin/.hubotrc",
    "ln -nfs ~/shared/bin/daemon.sh bin/daemon.sh"
  ],
  "run_cmd": "bin/daemon.sh restart"
}
